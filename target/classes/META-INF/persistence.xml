<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="enade" transaction-type="RESOURCE_LOCAL">
    <class>com.mycompany.enade.model.Resultado</class>
    <class>com.mycompany.enade.model.Usuario</class>
    <class>com.mycompany.enade.model.Tipoquestao</class>
    <class>com.mycompany.enade.model.Questao</class>
    <class>com.mycompany.enade.model.Tipousuario</class>
    <class>com.mycompany.enade.model.Prova</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/enade?autoReconnect=true&amp;useSSL=false"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="admin"/>
    </properties>
  </persistence-unit>
</persistence>
